generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}


model IDE {
  id      Int      @id @default(autoincrement())
  ideName    String @map("ide_name")
  containerIP String @map("container_ip")
}